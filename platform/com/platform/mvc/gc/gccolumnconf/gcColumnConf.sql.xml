<?xml version="1.0" encoding="UTF-8"?>

<sql-statement namespace="platform.gcColumnConf">
	
	<!-- 分页查询：from -->
	<sql id="splitPageFrom">
		<![CDATA[ 
        	from 
        		pt_fun_gc_columnconf
			
			where 1=1 
				
    	]]>
	</sql>
	<sql id="selectPageCounfByTnameAndCname">
		<![CDATA[ 
        	SELECT
				*
			FROM
				pt_fun_gc_columnconf
			WHERE
				 tableName = ?
			AND  columnName = ?
    	]]>
	</sql>
</sql-statement>
