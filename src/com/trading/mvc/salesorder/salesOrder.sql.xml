<?xml version="1.0" encoding="UTF-8"?>

<sql-statement namespace="trading.salesOrder">

	<!-- 分页查询：from -->
	<sql id="splitPageFrom">
		<![CDATA[ 
        	from 
        		b_trading_salesorder
			where 1=1 
    	]]>
	</sql>
	<sql id="selectByPocIds">
		<![CDATA[ 
        	SELECT
				*
			FROM
				b_trading_salesorder
			WHERE
				pocIds = ?;
    	]]>
	</sql>
</sql-statement>
